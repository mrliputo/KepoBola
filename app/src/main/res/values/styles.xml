<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <!-- Base application theme. -->
    <style name="FirstTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/homeColorPrimary</item>
        <item name="colorPrimaryDark">@color/homeColorPrimaryDark</item>
        <item name="colorAccent">@color/homeColorAccent</item>
    </style>

    <!-- Base application theme. -->
    <style name="ProfileTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/profileColorPrimary</item>
        <item name="colorPrimaryDark">@color/profileColorPrimaryDark</item>
        <item name="colorAccent">@color/profileColorAccent</item>
    </style>



</resources>